<?xml version="1.0" encoding="utf-8"?> 
<AutoVisualizer xmlns="http://schemas.microsoft.com/vstudio/debugger/natvis/2010">
  <Type Name="Cardinal::BaseLazy&lt;*,*&gt;">
    <DisplayString Condition="This->value != 0">{{Value : {*This->value}}}</DisplayString>
    <DisplayString Condition="This->value == 0">{{Value not loaded}}</DisplayString>
    <Expand>
      <!--<Item Name="Getter">This->getter</Item>-->
      <Item Name="Value">This->value</Item>
    </Expand>
  </Type>
</AutoVisualizer>